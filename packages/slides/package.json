{
  "name": "slides",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm build:ar1205 && pnpm build:ar2205",
    "build:ar1205": "slidev build slides.ar1205.md --base /ar1205/slides/ --out dist/ar1205",
    "build:ar2205": "slidev build slides.ar2205.md --base /ar2205/slides/ --out dist/ar2205",
    "dev": "echo 'Use dev:ar1205 or dev:ar2205'",
    "dev:ar1205": "slidev slides.ar1205.md --open",
    "dev:ar2205": "slidev slides.ar2205.md --open",
    "export": "slidev export"
  },
  "dependencies": {
    "@slidev/cli": "^52.10.1",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "markdown-it-implicit-figures": "^0.12.0",
    "slidev-addon-excalidraw": "^1.0.4",
    "slidev-theme-neversink": "^0.4.0",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "vite": "^7.3.1",
    "vue": "^3.5.25"
  }
}